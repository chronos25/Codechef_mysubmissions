#include<iostream>
 
using namespace std;
int main()
{
	int t,n,p,co,ci;
	
	cin>>t;
	while(t--)
	{
		cin>>n>>p;
		int a;
		co=0;
		ci=0;
		for(int i=0;i<n;i++)
		{
			cin>>a;
			if(a<=(p/10))
		      co++;
		    if(a>=(p/2))
		      ci++;
		}
		if((co==2)&&(ci==1))
		{
			cout<<"yes"<<endl;
		}
			else
			{
			cout<<"no"<<endl;
			}		
			
		}
		return 0;
}
