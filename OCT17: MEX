#include<bits/stdc++.h>
using namespace std;
int main()
{
	int t;
	long long int n,k;
	cin>>t;
	while(t--)
	{ k=0;
		cin>>n>>k;
		long long int a[n];
		for(int i=0;i<n;i++)
		{cin>>a[i];}
		sort(a,a+n);
		if(k==0)
		{
			for(int i=0;;i++)
			{if(a[i]!=i)
			 { cout<<i<<endl;
			 break;}
	      	}
		
		}
}
return(0);} 
