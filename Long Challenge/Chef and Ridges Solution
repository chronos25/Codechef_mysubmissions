#include<bits/stdc++.h>
//#include<conio.h>
using namespace std;
main()
{
	int t;
	cin>>t;
	int a[]={1,1,3,5,11,21,43,85,171,341,683,1365,2731,5461,10923,21845,43691,87381,174763,349525,699051,1398101,2796203,5592405,11184811,22369621};
	while(t--)
	{
		int n,x=0,y=0;
		cin>>n;
		y=pow(2,n);
		x=a[n-1];
		cout<<x<<" "<<y<<" ";		
	}
	//getch();
}
