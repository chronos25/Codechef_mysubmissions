#include<bits/stdc++.h> 
#define l long long int
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
	 l n,a,b;
	 cin>>n>>a>>b;
	 l arr[n],a1=0,b1=0;
	 for(l i=0;i<n;i++)
		 {
			cin>>arr[i];	
			if((arr[i]%a)==0)
			{ 
				a1+=1;
			}
		    else if((arr[i]%b)==0)
			{ 
				b1+=1;
			}		
		 } 
		 if(a1>b1)
		 {cout<<"BOB"<<endl;}
		 else
		 {cout<<"ALICE"<<endl;}
	 
    }
    //getch(rrsssrsrsr);
}
